{
  "explorer.compactFolders": false,
  "files.exclude": {
    "[Ll]ibs/": true,
    "[Pp]lugins/": true
  },
  "markdownlint.config": {
    "MD024": {
      "allow_different_nesting": true
    },
    "MD042": false,
    "no-inline-html": {
      "allowed_elements": [
        "img",
        "p"
      ]
    }
  },
  "cSpell.words": [
    "behaviour",
    "Behaviour",
    "cref",
    "dotnet",
    "unserialized"
  ]
}
